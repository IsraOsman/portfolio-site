{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portofolio page ",
  "main": "index.js",
  "engines": {
    "npm": "6.4.1",
    "node": "10.15.3"
  },
  "scripts": {
    "deploy": "gh-pages -d dist",
    "watch:sass": "node-sass dist/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass dist/sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass concat:css deploy"
  },
  "author": "Isra",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  },
  "homepage": "https://IsraOsman.github.io/portfolio-site",
  "dependencies": {
    "gh-pages": "^3.1.0",
    "postcss-cli": "^7.1.1"
  }
}